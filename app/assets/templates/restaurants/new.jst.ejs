<div class="row">
	<div class="col-md-3">
	</div>

	<div class="col-md-5">

	<h2>List a New Location<h2>

	<form class="form-horizontal" id="rest-form">
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_name">Name</label>
	    <input type="text" class="form-control" name="restaurant[name]" id="restaurant_name" placeholder="Location Name">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_line1">Address Line 1</label>
	    <input type="text" class="form-control" name="restaurant[address_attributes][line1]" id="restaurant_line1" placeholder="Address Line 1">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_line2">Address Line 2</label>
	    <input type="text" class="form-control" name="restaurant[address_attributes][line2]" id="restaurant_line2" placeholder="Address Line 2">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_city">City</label>
	    <input type="text" class="form-control" name="restaurant[address_attributes][city]" id="restaurant_city" placeholder="City">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_state">State</label>
	    <input type="text" class="form-control" name="restaurant[address_attributes][state]" id="restaurant_state" placeholder="State">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_zip">Zip</label>
	    <input type="text" class="form-control" name="restaurant[address_attributes][zip]" id="restaurant_zip" placeholder="Zip">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_phone">phone</label>
	    <input type="text" class="form-control" name="restaurant[phone]" id="restaurant_phone" placeholder="Phone">
	  </div>
	  <div class="form-group">
	    <label class="sr-only" for="restaurant_url">URL</label>
	    <input type="text" class="form-control" name="restaurant[url]" id="restaurant_url" placeholder="URL">
	  </div>
		<div class="col-md-7">
		</div>
		<div class="col-md-5">
			<button id="submit-new" class="btn btn-warning btn-block">Submit!</button>
		</div>
	</form>
	</div>
</div>